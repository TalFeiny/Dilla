{
  "name": "dilla-ai",
  "version": "1.0.0",
  "description": "Dilla AI - VC Platform",
  "scripts": {
    "dev": "npm run dev:frontend",
    "dev:frontend": "cd frontend && npm run dev -- -p 3001",
    "dev:backend": "cd backend && source venv/bin/activate && uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "test": "npm run test:backend",
    "test:backend": "cd backend && pytest",
    "test:frontend": "cd frontend && npm test",
    "check:db": "ts-node check_db_counts.ts",
    "check:simple": "ts-node simple_count_check.ts",
    "verify": "ts-node verify_connections.ts",
    "test:middleware": "ts-node test_middleware.ts",
    "build:ts": "tsc",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "vc",
    "venture-capital",
    "ai",
    "fastapi",
    "nextjs"
  ],
  "author": "Dilla AI",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.11",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "node-fetch": "^2.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0"
  }
}
